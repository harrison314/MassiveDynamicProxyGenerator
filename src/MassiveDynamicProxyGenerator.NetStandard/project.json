{
  "version": "1.0.0-*",
  "buildOptions": {
    "define": [ "COREFX" ],
    "xmlDoc": true,
    "compile": {
      "include": [ "**/*.cs", "../MassiveDynamicProxyGenerator/**/*.cs" ]
    },
    "outputName": "MassiveDynamicProxyGenerator"
  },

  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE" ]
      }
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "System.Reflection.Emit": "4.3.0",
    "System.Dynamic.Runtime": "4.3.0"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  }
}
